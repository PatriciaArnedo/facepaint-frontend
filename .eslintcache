rFilters = () => {\n        if (this.props.userObj.filters && this.props.userObj.filters.length) {\n            const filteredFilters = this.props.userObj.filters.filter(filter => filter.name.toLowerCase().includes(this.state.searchTerm.toLowerCase()))\n            return filteredFilters.map(filterObj => <FilterCard key={filterObj.id} filterObj={filterObj} belongsToUser={false} isSavedFilter={false} isUserCard={true} />)\n        } else {\n            return <p>This User Hasn't Created Any Filters Yet</p>\n        }\n    }\n\n    renderSavedFilters = () => {\n        if (this.props.userObj.save_filters && this.props.userObj.save_filters.length) {\n            const filteredFilters = this.props.userObj.save_filters.filter(filter => filter.name.toLowerCase().includes(this.state.searchTerm.toLowerCase()))\n            return filteredFilters.map(filterObj => <FilterCard key={filterObj.id} filterObj={filterObj} belongsToUser={false} isSavedFilter={false} isUserCard={true} />)\n        } else {\n            return <p>This User Hasn't Saved Any Filters Yet</p>\n        }\n    }\n\n    searchOnChange = (e) => {\n        this.setState({ [e.target.name]: e.target.value })\n    }\n\n    render() {\n        return (\n            <div className=\"user-profile\">\n                <div className=\"profile-header\">\n                    <img src={this.props.userObj.avatar ? this.props.userObj.avatar : \"https://i.imgur.com/igyvLpE.jpg\" } className=\"profile-avatar\" />\n                <div className=\"user-details\">\n                    <h3>@{this.props.userObj.username}</h3>\n                    <p>{this.props.userObj.name}</p>\n                    {this.props.userObj.instagram ? <p>instagram: {this.props.userObj.instagram}</p> : null}\n                    <p>{this.props.userObj.bio}</p>\n                    <h5>{this.props.userObj.save_count} Total Saves</h5>\n                <br />\n                <input\n                    id=\"form-input\"\n                    type=\"text\"\n                    value={this.state.searchTerm}\n                    onChange={this.searchOnChange}\n                    name=\"searchTerm\"\n                    placeholder=\"Search Filters by Name\"\n                />\n                </div>\n                </div>\n                <br />\n                <TabView>\n                    <TabPanel header={`Filters`} leftIcon=\"pi pi-fw pi-palette\">\n                        <div className=\"profile-filters\">\n                            {this.renderFilters()}\n                        </div>\n                    </TabPanel>\n                    <TabPanel header=\"Saved Filters\" leftIcon=\"pi pi-fw pi-bookmark\">\n                        <div className=\"profile-filters\">\n                            {this.renderSavedFilters()}\n                        </div>\n                    </TabPanel>\n                </TabView>\n            </div>\n        )\n    }\n\n}\n\nexport default UserProfile","/Users/Patricia/Documents/Flatiron/code/mod5/mod5-proj/facepaint-frontend/src/index.js",[],"/Users/Patricia/Documents/Flatiron/code/mod5/mod5-proj/facepaint-frontend/src/account/EditAccount.js",["133"],"/Users/Patricia/Documents/Flatiron/code/mod5/mod5-proj/facepaint-frontend/src/containers/UserContainer.js",[],["134","135"],"/Users/Patricia/Documents/Flatiron/code/mod5/mod5-proj/facepaint-frontend/src/filterfiles/SavedFilters.js",[],"/Users/Patricia/Documents/Flatiron/code/mod5/mod5-proj/facepaint-frontend/src/filterfiles/FilterCard.js",[],"/Users/Patricia/Documents/Flatiron/code/mod5/mod5-proj/facepaint-frontend/src/filterfiles/SaveFilterForm.js",[],"/Users/Patricia/Documents/Flatiron/code/mod5/mod5-proj/facepaint-frontend/src/camerafiles/FaceFilterCanvas.js",[],"/Users/Patricia/Documents/Flatiron/code/mod5/mod5-proj/facepaint-frontend/src/account/SignUp.js",[],"/Users/Patricia/Documents/Flatiron/code/mod5/mod5-proj/facepaint-frontend/src/account/UserCard.js",[],"/Users/Patricia/Documents/Flatiron/code/mod5/mod5-proj/facepaint-frontend/src/account/ImageUpload.js",["136","137"],"import React from 'react';\nimport ReactCrop from 'react-image-crop';\nimport 'react-image-crop/dist/ReactCrop.css';\nimport { connect } from 'react-redux'\nimport { getUser, uploadAvatar } from '../redux/actions'\n\n\nclass ImageUpload extends React.Component {\n    state = {\n        src: null,\n        crop: {\n            unit: '%',\n            width: 30,\n            aspect: 1 / 1\n        },\n    };\n\n    onSelectFile = e => {\n        if (e.target.files && e.target.files.length > 0) {\n            const reader = new FileReader();\n            reader.addEventListener('load', () =>\n                this.setState({ src: reader.result })\n            );\n            reader.readAsDataURL(e.target.files[0]);\n        }\n    };\n\n    // If you setState the crop in here you should return false.\n    onImageLoaded = image => {\n        this.imageRef = image;\n    };\n\n    onCropComplete = crop => {\n        this.makeClientCrop(crop);\n    };\n\n    onCropChange = (crop, percentCrop) => {\n        // You could also use percentCrop:\n        // this.setState({ crop: percentCrop });\n        this.setState({ crop });\n    };\n\n\n\n    async makeClientCrop(crop) {\n        if (this.imageRef && crop.width && crop.height) {\n            const croppedImageUrl = this.getCroppedImg(\n                this.imageRef,\n                crop,\n                'newFile.jpeg'\n            );\n            this.setState({ croppedImageUrl });\n        }\n    }\n\n    getCroppedImg(image, crop, fileName) {\n        const canvas = document.createElement('canvas');\n        const scaleX = image.naturalWidth / image.width;\n        const scaleY = image.naturalHeight / image.height;\n        canvas.width = crop.width;\n        canvas.height = crop.height;\n        const ctx = canvas.getContext('2d');\n\n        ctx.drawImage(\n            image,\n            crop.x * scaleX,\n            crop.y * scaleY,\n            crop.width * scaleX,\n            crop.height * scaleY,\n            0,\n            0,\n            crop.width,\n            crop.height\n        );\n\n        const base64Image = canvas.toDataURL('image/jpeg')\n\n        return base64Image\n        // return new Promise((resolve, reject) => {\n        //   canvas.toBlob(blob => {\n        //     if (!blob) {\n        //       //reject(new Error('Canvas is empty'));\n        //       console.error('Canvas is empty');\n        //       return;\n        //     }\n        //     blob.name = fileName;\n        //     window.URL.revokeObjectURL(this.fileUrl);\n        //     this.fileUrl = window.URL.createObjectURL(blob);\n        //     resolve(this.fileUrl);\n        //   }, 'image/jpeg');\n        // });\n    }\n\n    imgSaveHandler = () => {\n        let imgObj = {\n            avatar: this.state.croppedImageUrl\n        }\n        this.props.uploadAvatar(imgObj, this.props.userId)\n        this.setState({\n            src: null,\n            croppedImageUrl: null\n        })\n    }\n\n    render() {\n        const { crop, croppedImageUrl, src } = this.state;\n\n        return (\n            <div style={{ textAlign: \"center\" }} className=\"ImageUpload\">\n                <div>\n                    <input type=\"file\" accept=\"image/*\" onChange={this.onSelectFile} />\n                </div>\n                {src && (\n                    <div style={{ maxWidth: \"400px\" }}>\n                        <ReactCrop\n                            src={src}\n                            crop={crop}\n                            ruleOfThirds\n                            onImageLoaded={this.onImageLoaded}\n                            onComplete={this.onCropComplete}\n                            onChange={this.onCropChange}\n                            circularCrop={true}\n                            ruleOfThirds={false}\n                        />\n                        <br />\n                    </div>\n                )}\n                {croppedImageUrl && (\n                    <img alt=\"Crop\" style={{ maxWidth: '100%', borderRadius: \"50%\" }} src={croppedImageUrl} />\n                )}\n                <br />\n                {this.state.src ?\n                    <button onClick={this.imgSaveHandler}>Save</button>\n                    :\n                    null\n                }\n            </div>\n        );\n    }\n}\n\nfunction msp(state) {\n    return {\n        user: state.user,\n        userId: state.userId,\n        userObj: state.userObj\n    }\n}\n\nfunction mdp(dispatch) {\n    return {\n        uploadAvatar: (imgObj, userId) => dispatch(uploadAvatar(imgObj, userId)),\n    }\n}\n\nexport default connect(msp,mdp)(ImageUpload)\n","/Users/Patricia/Documents/Flatiron/code/mod5/mod5-proj/facepaint-frontend/src/redux/reducer.js",[],"/Users/Patricia/Documents/Flatiron/code/mod5/mod5-proj/facepaint-frontend/src/filterfiles/ExploreFilters.js",[],"/Users/Patricia/Documents/Flatiron/code/mod5/mod5-proj/facepaint-frontend/src/camerafiles/FilterTryOn.js",[],{"ruleId":"138","replacedBy":"139"},{"ruleId":"140","replacedBy":"141"},{"ruleId":"142","severity":1,"message":"143","line":51,"column":21,"nodeType":"144","endLine":51,"endColumn":178},{"ruleId":"138","replacedBy":"145"},{"ruleId":"140","replacedBy":"146"},{"ruleId":"138","replacedBy":"147"},{"ruleId":"140","replacedBy":"148"},{"ruleId":"138","replacedBy":"149"},{"ruleId":"140","replacedBy":"150"},{"ruleId":"138","replacedBy":"151"},{"ruleId":"140","replacedBy":"152"},{"ruleId":"138","replacedBy":"153"},{"ruleId":"140","replacedBy":"154"},{"ruleId":"142","severity":1,"message":"143","line":37,"column":21,"nodeType":"144","endLine":37,"endColumn":152},{"ruleId":"142","severity":1,"message":"143","line":30,"column":17,"nodeType":"144","endLine":30,"endColumn":147},{"ruleId":"138","replacedBy":"155"},{"ruleId":"140","replacedBy":"156"},{"ruleId":"157","severity":1,"message":"158","line":5,"column":10,"nodeType":"159","messageId":"1